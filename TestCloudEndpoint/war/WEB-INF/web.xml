<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<servlet>
		<servlet-name>FBCheckEndpoint</servlet-name>
		<servlet-class>osg.service.endpoints.FBCheckEndpoint</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FBCheckEndpoint</servlet-name>
		<url-pattern>/fbcheckendpoint</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FBStreamEndpoint</servlet-name>
		<servlet-class>osg.service.endpoints.FBStreamEndpoint</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FBStreamEndpoint</servlet-name>
		<url-pattern>/fbstreamendpoint</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value>osg.service.endpoints.DatastoreServices</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>