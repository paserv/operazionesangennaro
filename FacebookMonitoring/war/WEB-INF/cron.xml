<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
	<cron>
		<url>/facebook/monitoring/trasmissioni</url>
		<description>Monitor Facebook Pages contained into trasmissioni table every 30 minutes
		</description>
		<schedule>every 30 minutes synchronized</schedule>
	</cron>
	<cron>
		<url>/facebook/monitoring/sindaci</url>
		<description>Monitor Facebook Pages contained in sindaco table every 6 hours
		</description>
		<schedule>every 6 hours synchronized</schedule>
	</cron>
	<cron>
		<url>/deletetasklog</url>
		<description>Delete Task Log in table task every 24 hours
		</description>
		<schedule>every day 03:00</schedule>
	</cron>
	<cron>
		<url>/deletenodelog</url>
		<description>Delete Node Log in table task every 1 hours
		</description>
		<schedule>every day 04:00</schedule>
	</cron>
	<cron>
		<url>/deleteedgelog</url>
		<description>Delete Edge Log in table task every 1 hours
		</description>
		<schedule>every day 05:00</schedule>
	</cron>
</cronentries>